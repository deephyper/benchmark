
VERSION=0.0.1

build:
	mkdir -p build
	wget https://github.com/hepnos/HEPnOS-Autotuning-analysis/archive/refs/heads/main.tar.gz -O build/hepnos-autotuning-analysis.tar.gz
	tar -xzvf build/hepnos-autotuning-analysis.tar.gz -C build
	rm -rf build/hepnos-autotuning-analysis.tar.gz

clean:
	rm -rf build/